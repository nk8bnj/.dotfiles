{
  "vim.leader": "<space>",
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.handleKeys": {
    "<C-c>": false,
    "<C-[>": false,
    "<C-w>": false,
    "<C-z>": false
  },
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<Esc>"],
      "when": ["editorFocus"],
      "commands": [":nohl"],
      "silet": true
    }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "workbench.colorCustomizations": {
    "sideBar.border": "#3B4347",
    "statusBar.border": "#3B4347",
    "statusBarItem.remoteBackground": "#A7C081",
    "statusBarItem.remoteForeground": "#2D353B",
    "activityBar.border": "#3B4347",
    "sideBar.background": "#2C3339",
    "panel.border": "#3B4347",
    "panel.background": "#2C3339",
    "editorLineNumber.activeForeground": "#a1adab",
    "editorCursor.foreground": "#778280",
    "editorCursor.background": "#000000",
    "activityBarBadge.background": "#A7C082",
    "tab.border": "#3B4347",
    "tab.activeBorderTop": "#A7C082",
    "tab.activeBorder": "#2D353B",
    "editorGroupHeader.tabsBorder": "#3B4347",
    "titleBar.border": "#3B4347",
    "editorGroupHeader.tabsBackground": "#2C3339",
    "tab.inactiveBackground": "#2C3339",
    "widget.shadow": "#ffffff00",
    "scrollbar.shadow": "#ffffff00",
    "tab.unfocusedActiveBorder": "#2D353B"
  },
  "breadcrumbs.enabled": false,
  "window.openFilesInNewWindow": "off",
  "window.restoreWindows": "none",
  "editor.insertSpaces": false,
  "editor.smoothScrolling": true,
  "editor.minimap.enabled": false,
  "editor.detectIndentation": true,
  "editor.suggestSelection": "first",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.wrappingIndent": "same",
  "editor.scrollBeyondLastLine": true,
  "editor.linkedEditing": true,
  "html.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "typescript.autoClosingTags": true,
  "editor.renderControlCharacters": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  // "workbench.productIconTheme": "fluent-icons",
  "window.density.editorTabHeight": "default",
  "editor.accessibilitySupport": "off",
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.editor.empty.hint": "hidden",
  "editor.fontSize": 15,
  "editor.lineHeight": 23,
  "editor.fontWeight": "400",
  "editor.fontLigatures": true,
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.inlayHints.fontFamily": "FiraCode Nerd Font",
  "chat.editor.fontFamily": "FiraCode Nerd Font",
  "editor.codeLensFontFamily": "FiraCode Nerd Font",
  "debug.console.fontFamily": "FiraCode Nerd Font",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "entity.name.type.class",
          "keyword",
          "constant",
          "storage.modifier",
          "storage.type.class.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorStyle": "line",
  "workbench.editor.tabSizing": "shrink",
  "debug.toolBarLocation": "hidden",
  "debug.focusWindowOnBreak": false,
  "debug.showInlineBreakpointCandidates": false,
  "debug.showBreakpointsInOverviewRuler": false,
  "emmet.includeLanguages": {
    "blade": "html",
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "prettier.arrowParens": "avoid",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.inlineSuggest.enabled": true,
  "breadcrumbs.icons": false,
  "breadcrumbs.showKeys": false,
  "breadcrumbs.showFiles": false,
  "breadcrumbs.symbolPath": "off",
  "breadcrumbs.showArrays": false,
  "breadcrumbs.showEvents": false,
  "breadcrumbs.showFields": false,
  "breadcrumbs.showClasses": false,
  "breadcrumbs.showMethods": false,
  "breadcrumbs.showBooleans": false,
  "breadcrumbs.showFunctions": false,
  "breadcrumbs.showConstants": false,
  "breadcrumbs.showEnumMembers": false,
  "breadcrumbs.showConstructors": false,
  "files.defaultLanguage": "plaintext",
  "diffEditor.ignoreTrimWhitespace": false,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.stickyScroll.scrollWithEditor": false,
  "workbench.tree.enableStickyScroll": false,
  "editor.stickyScroll.enabled": false,
  "window.commandCenter": false,
  "markdown.updateLinksOnFileMove.enabled": "always",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.tips.enabled": false,
  "vscode_custom_css.imports": [
    "file:///Users/Nikita/.config/vscode/custom-vscode.css"
  ],
  "editor.matchBrackets": "never",
  "window.title": "${folderPath}",
  "editor.scrollbar.verticalScrollbarSize": 0,
  // "scm.diffDecorations": "none",
  // "scm.diffDecorationsGutterAction": "none",
  "cSpell.autoFormatConfigFile": true,
  "json.format.enable": true,
  "json.format.keepLines": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "cSpell.userWords": [
    "AERIN",
    "agentdeskai",
    "animejs",
    "cicd",
    "Coef",
    "Coeff",
    "coefs",
    "datatable",
    "endcomment",
    "endfor",
    "endschema",
    "enlargable",
    "expt",
    "expts",
    "fetchpriority",
    "fieldset",
    "Filippo",
    "flac",
    "Flickity",
    "forloop",
    "Gridlines",
    "hbspt",
    "hubspot",
    "linecap",
    "linejoin",
    "marketwatch",
    "metafields",
    "metaobjects",
    "mobily",
    "nbsp",
    "neverthrow",
    "preorder",
    "Pressable",
    "primeng",
    "Qrok",
    "Scandi",
    "Shadcn",
    "SPOONACULAR",
    "standart",
    "stlz",
    "stolz",
    "stylesheet",
    "SUPABASE",
    "TDEE",
    "tecnical",
    "treetable",
    "Turati",
    "ungroup",
    "vaul",
    "vcgallery",
    "vercel",
    "Wearstler"
  ],
  "laravel-pint.enable": true,
  "workbench.activityBar.orientation": "vertical",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "workbench.colorTheme": "Everforest Dark",
  "editor.tabSize": 2,
  "[liquid]": {
    "editor.defaultFormatter": "sissel.shopify-liquid",
    "editor.formatOnSave": true
  },
  "window.openFoldersInNewWindow": "off",
  "window.zoomPerWindow": false,
  "window.zoomLevel": 0.3,
  "cursor.cpp.enablePartialAccepts": true,
  "cursor.composer.shouldChimeAfterChatFinishes": true
}

